
public class variablesAndDatatypes {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 5;
		String name = "vamsi";//string is non-primitive datatype
		char grade = 'A';
		double percen = 100.00;
		
		System.out.println(name +" passed exam with " + grade + " and "  + percen +  " percentile out of " + n + " students present");
		

	}

}
